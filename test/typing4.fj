type bool
data constructor True : {} -> bool
data constructor False : {} -> bool

type t
data constructor A : {} -> t
data constructor B : {} -> t
data constructor C : {} -> t


program
      let preserve (v: t)(big: bool): bool =
        join j (x: bool): bool =
          match big return bool with
          | True {} -> False {}
          | False {} -> True {}
          end
        in match v return bool with
           | A {} -> jump j { True {} } : bool
           | B {} -> jump j { False {} } : bool
           | C {} -> False {}
           end
      in
      preserve
