type bool
data constructor True : {} -> bool
data constructor False : {} -> bool
program
fun [a] (e1 : bool) (e2 : bool) (e3 : bool) (big4 : a) (big5 : a) : a = 
  join j (x : bool) : a = 
    match x return a with | True {} -> big4 | False {} -> big5 end
  in
  match e1 return a with
  | True {} ->
      (jump j {e2} : a)
  | False {} ->
      (jump j {e3} : a)
  end