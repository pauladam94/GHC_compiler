type list a
data constructor Nil : forall a. {} -> list a
data constructor Cons : forall a. { a; list a } -> list a
type bool
data constructor True : {} -> bool
data constructor False : {} -> bool
program
fun [a] [b] [c] (b : bool) (e1 : list b) (e2 : list b) (big1 : c) (big2 : b ->
  list b -> c) : c = 
  match match b return list b with | True {} -> e1 | False {} -> e2 end return
  c with
  | Nil [_] {} ->
      big1
  | Cons [_] { a; as } ->
      big2 a as
  end